generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // â†” Pour SQLite: "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
